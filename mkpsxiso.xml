      </dir>

      <!-- Lastly, some dummy sectors must always be placed at the -->
      <!-- end of the disc image. It must be a pretty large one to -->
      <!-- make sure the console does not go run-away when it -->
      <!-- reaches the end of the track -->
      <dummy sectors="1024"/>

    </directory_tree>

